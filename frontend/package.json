    }
  ],
  "keywords": [
    "pdf",
    "highlight",
    "annotator",
    "react-component",
    "react"
  ],
  "files": [
    "dist"
  ],
  "types": "./dist/esm/index.d.ts",
  "main": "./dist/cjs/index.js",
